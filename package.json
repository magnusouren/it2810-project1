{
  "name": "webdev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage --reporter=junit --outputFile.junit=./coverage/junit.xml",
    "build": "npm run lint && npm run lint:css && tsc && vite build",
    "build:ci": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,json .",
    "lint:css": "npx stylelint \"src/**/*{css, js, ts, tsx, jsx}\"",
    "lint:css:fix": "npx stylelint --fix \"src/**/*{css, js, ts, tsx, jsx}\"",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,scss,json,css}' --config .prettierrc.cjs"
  },
  "dependencies": {
    "@tanstack/query-sync-storage-persister": "^4.33.0",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-persist-client": "^4.33.0",
    "axios": "^1.5.0",
    "eslint-config-prettier": "^9.0.0",
    "msw": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest-image-snapshot": "^6.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest-image-snapshot": "^6.2.0",
    "jsdom": "^22.1.0",
    "nock": "^13.3.3",
    "postcss-less": "^6.0.0",
    "postcss-lit": "^1.1.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.3"
  }
}
